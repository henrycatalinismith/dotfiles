{
  "assistant": {
    "default_model": {
      "model": "o1",
      "provider": "copilot_chat"
    },
    "version": "2"
  },
  "buffer_font_family": "Iosevka",
  "buffer_font_size": 18,
  "git": {
    "inline_blame": {
      "min_column": 80,
      "show_commit_summary": true
    }
  },
  "indent_guides": {
    "enabled": false
  },
  "inline_completions_disabled_in": [
    "comment"
  ],
  "journal": {
    "hour_format": "hour24"
  },
  "languages": {
    "Markdown": {
      "inline_completions_disabled_in": [
        "code",
        "comment",
        "string"
      ]
    }
  },
  "scrollbar": {},
  "tab_bar": {
    "show_nav_history_buttons": false
  },
  "tab_size": 2,
  "tabs": {
    "activate_on_close": "neighbour",
    "git_status": true
  },
  "terminal": {
    "line_height": "standard"
  },
  "theme": {
    "dark": "Zed Legacy: Solarized Dark",
    "light": "Solarized Light",
    "mode": "dark"
  },
  "use_autoclose": false,
  "vim_mode": true
}
